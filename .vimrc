" this is comment
set number
" behave mswin
behave xterm
set hlsearch
set incsearch
set ignorecase
set smartcase
set guifont=DejaVu\ Sans\ Mono\ 9
set lines=64 columns=160
set cursorline
colorscheme evening  
set guioptions+=b 
filetype indent on
filetype plugin on
set tabstop=4
set shiftwidth=4 softtabstop=4
